    { "name": "endereco", "type": "text", "required": true },
    { "name": "numero", "type": "text", "required": true },
    { "name": "bairro", "type": "text", "required": true },
    { "name": "cidade", "type": "text", "required": true },
    { "name": "estado", "type": "text", "required": true },
