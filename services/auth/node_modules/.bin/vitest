#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel/node_modules/.pnpm/vitest@1.6.1_@types+node@20_942fea36cbc5f9eca851d74ac68297f6/node_modules/vitest/node_modules:/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel/node_modules/.pnpm/vitest@1.6.1_@types+node@20_942fea36cbc5f9eca851d74ac68297f6/node_modules:/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel/node_modules/.pnpm/vitest@1.6.1_@types+node@20_942fea36cbc5f9eca851d74ac68297f6/node_modules/vitest/node_modules:/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel/node_modules/.pnpm/vitest@1.6.1_@types+node@20_942fea36cbc5f9eca851d74ac68297f6/node_modules:/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
