#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel-umadeus/node_modules/.pnpm/btoa@1.2.1/node_modules/btoa/bin/node_modules:/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel-umadeus/node_modules/.pnpm/btoa@1.2.1/node_modules/btoa/node_modules:/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel-umadeus/node_modules/.pnpm/btoa@1.2.1/node_modules:/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel-umadeus/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel-umadeus/node_modules/.pnpm/btoa@1.2.1/node_modules/btoa/bin/node_modules:/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel-umadeus/node_modules/.pnpm/btoa@1.2.1/node_modules/btoa/node_modules:/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel-umadeus/node_modules/.pnpm/btoa@1.2.1/node_modules:/mnt/c/Users/jonat/OneDrive/Documentos/GitHub/QG3/admin-panel-umadeus/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../btoa@1.2.1/node_modules/btoa/bin/btoa.js" "$@"
else
  exec node  "$basedir/../../../../../btoa@1.2.1/node_modules/btoa/bin/btoa.js" "$@"
fi
